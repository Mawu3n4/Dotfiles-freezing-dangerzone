rxvt*color9:			#cf6171
rxvt*color10:			#c5f779
rxvt*color11:			#fff796
rxvt*color12:			#4186be
rxvt*color13:			#cf9ebe
rxvt*color14:			#71bebe
rxvt*color15:			#ffffff

/* emacs */
emacs*font:			7x13bold
emacs*ReverseVideo:		on

