
!Colors

! tangoesque scheme
*background: #111111
*foreground: #babdb6
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff

/* xterm */
xterm*font:                     9x15bold
xterm*background:               black
xterm*foreground:               white
xterm*saveLines:                1000
xterm*scrollBar:                false
xterm*geometry:           80x25
xterm*faceName:           terminus:bold:pixelsize=11
xterm*font:              -*-comicneue-light-r-*-*-14-*5-*-*-*-*-*-*
xterm*dynamicColors:      true
xterm*utf8:               1

xterm*saveLines:          1024
xterm*scrollTtyKeypress:  true
xterm*scrollTtyOutput:    false
xterm*scrollBar:          false
XTerm*transparent: true
xterm*rightScrollBar:     false
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*toolBar:            false
xterm*metaSendsEscape:    true
xterm*eightBitInput:      false
xterm*perl-ext-common:          default,matcher
xterm*urlLauncher:              /usr/bin/google-chrome
xterm*matcher.button:           1

/* emacs */
emacs*font:			7x13bold
emacs*ReverseVideo:		on

xterm*faceNameDoublesize: TakaoExMincho
xterm*faceSize: 10